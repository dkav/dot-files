# PATH
# ----
# Base PATH
path=(/usr/local/bin
      /usr/bin
      /bin
      /usr/sbin
      /sbin)

# Paths to additional binaries
path+=(/Library/Frameworks/GDAL.framework/Versions/Current/Programs)
path+=(/Applications/Postgres.app/Contents/Versions/latest/bin)

export PATH


# DEVELOPEMENT
# ------------
# Add functions that setup dev environments
if [ -f ~/.dev_setup ]; then
    source ~/.dev_setup
fi
