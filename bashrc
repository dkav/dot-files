# ALIASES
# -------
# 'ls'  Aliases
alias ll="ls -lh"
alias la="ls -lah"
alias lh="ls -lad .*"
alias ls="ls -G"

# Windows Aliases
alias cls="clear"
alias dir="ls"

# Development Aliases
alias ddd="cd /Users/Darren/Documents/Development"
alias pws="python -m SimpleHTTPServer &"
alias ipn="ipython notebook"

